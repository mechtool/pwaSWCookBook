{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/cacheStrategy",
        "function": "cacheStrategy"
      },
	  {
		"source": "/immediateClaim",
		"function": "immediateClaim"
	  },

      {
      "source": "**",
      "destination": "/index.html"
    } ]
  }
}
